{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "lib": ["es2020", "dom"],
    "types": [
      "cypress",
      "node",
      "cypress-xpath",
      "cypress-file-upload",
      "cypress-wait-until"
    ],
    "moduleResolution": "node",
    "typeRoots": ["./support"],
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": ["**/*.ts"]
}
